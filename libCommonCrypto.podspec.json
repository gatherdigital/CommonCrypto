{
  "name": "libCommonCrypto",
  "version": "0.1.1",
  "license": "MIT",
  "summary": "CommonCrypto module maps for swift",
  "homepage": "https://github.com/mogstad/CommonCrypto",
  "authors": {
    "Bjarne Mogstad": "me@mogstad.co"
  },
  "source": {
    "git": "https://github.com/mogstad/CommonCrypto.git",
    "tag": "v0.1.1"
  },
  "module_name": "CommonCrypto",
  "platforms": {
    "ios": "8.0",
    "osx": "10.9"
  },
  "ios": {
    "module_map": "libCommonCrypto/module.modulemap"
  },
  "tvos": {
    "module_map": "libCommonCrypto/module.modulemap"
  },
  "osx": {
    "module_map": "libCommonCrypto/module.modulemap"
  },
  "watchos": {
    "module_map": "libCommonCrypto/module.modulemap"
  },
  "xcconfig": { "HEADER_SEARCH_PATHS": "$(SDKROOT)/usr/include/CommonCrypto" },
  "frameworks": "Security",
  "source_files": ["libCommonCrypto/*.h", "libCommonCrypto/*.swift"],
  "requires_arc": true
